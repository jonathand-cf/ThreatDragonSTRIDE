                {
                  "id": "tb-01",
                  "title": "Direct Internet exposure of OT network",
                  "status": "Open",
                  "severity": "Critical",
                  "type": "Elevation of Privilege",
                  "description": "The SCADA environment is reachable from outside without strict network isolation, allowing remote attackers to directly reach OT assets.",
